<mvc:View
	controllerName="sap.dm.controller.SuccessStories"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:control="control"
	xmlns:h="http://www.w3.org/1999/xhtml">
	<Page
		title="Success Story"
		navButtonPress="handleNavBack"
		showNavButton="{device>/isPhone}"
		showFooter="{device>/isNoPhone}">
		<headerContent>
		</headerContent>
		<footer>
			<Toolbar>
			</Toolbar>
		</footer>
		<content>
			<Carousel
				pageChanged="onPageChanged"
				class="sapUiContentPadding"
				loop="true">
				<pages>
					<Image
						id="idImgINPEX"
						src="img/successStories/logo1.png"
						alt="INPEX" />
					<Image
						id="idImgHILTI"
						src="img/successStories/logo2.png"
						alt="HILTI" />
					<Image
						id="idImgPOSTNL"
						src="img/successStories/logo3.png"
						alt="POSTNL" />
					<Image
						id="idImgSHFA"
						src="img/successStories/logo4.png"
						alt="SHFA" />
				</pages>
			</Carousel>
		</content>
	</Page>
</mvc:View>